<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.uniquindio.stressapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_stressapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_stressapp_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StressApp">
        <activity
            android:name=".ReportesActivity"
            android:exported="true" />
        <activity
            android:name=".FuentesActivity"
            android:exported="true" />
        <activity
            android:name=".CalificacionActivity"
            android:exported="true" />
        <activity
            android:name=".AvanzadosActivity"
            android:exported="true" />
        <activity
            android:name=".IntermediosActivity"
            android:exported="true" />
        <activity
            android:name=".NovatosActivity"
            android:exported="true" />
        <activity
            android:name=".EjerciciosActividadFisicaActivity"
            android:exported="true" />
        <activity
            android:name=".EjerciciosMeditacionActivity"
            android:exported="true" />
        <activity
            android:name=".EjerciciosRespiracionActivity"
            android:exported="true" />
        <activity
            android:name=".EjerciciosMusicoterapiaActivity"
            android:exported="true" />
        <activity
            android:name=".VideoPlayerActivity"
            android:exported="true" />
        <activity
            android:name=".InformacionRecursosActivity"
            android:exported="true" />
        <activity
            android:name=".MenuRecursosActivity"
            android:exported="true" />
        <activity
            android:name=".EncuestaUnoActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EncuestaDosActivity"
            android:exported="true" />
    </application>

</manifest>